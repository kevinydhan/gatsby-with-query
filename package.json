  "author": "Kevin Han <kevinhanyd@gmail.com>",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "homepage": "https://github.com/kevinydhan/gatsby-with-query",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinydhan/gatsby-with-query.git"
  },
